code:
if = { limit = { AND = { TAG = ARR OVERLORD = { has_country_flag = enclave_nation }}} set_cosmetic_tag = ARR_ENC set_country_flag = color_changed }

owns_state = 39 -- Checks if the country owns the state with the given number as state id.

has_cosmetic_tag = SOV_custom -- Checks if the current scope has the specified cosmetic tag active.

add_core_of = SOV -- Adds a core for the specified country on the current scope.

transfer_state = 345 -- Give control ower state

set_capital = 345 -- Makes the specified state the current scope's capital state.

211 = { set_state_name = "Nova Roma" } -- Rename city

custom_trigger_tooltip = {
    tooltip = sunrise_invasion_tt		
    any_state = {
        is_owned_by = JAP
        is_on_continent = europe
        is_coastal = yes
    }
}

custom_effect_tooltip = 11_SOV_tt

11_SOV_tt: "Spread communist support to all neighbouring countries.\n"

